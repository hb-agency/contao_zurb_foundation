{
    "name": "hb-agency/contao_zurb_foundation",
    "description": "A set of tools for Contao Open Source CMS to utilize the Foundation responsive framework",
    "keywords": [
    	"zurb", 
    	"foundation"
    ],
    "type": "contao-module",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "HB Agency",
            "homepage": "http://www.hbagency.com"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "contao/core": ">=3.3.0,<4.0.0-dev",
        "contao-community-alliance/composer-installer": "*",
        "contao-legacy/namespaceclassloader": ">=1.0.1",
        "contao-legacy/haste": ">=4.0.0,<5.0-dev",
        "menatwork/contao-multicolumnwizard": ">=3.2.0,<3.3-dev",
        "zurb/foundation": ">=5.2.1,<5.5-dev",
        "jalle19/php-yui-compressor": ">=dev-master",
        "tedivm/jshrink": ">=1.0.0,<1.1-dev",
        "components/jquery": ">=2.1.0,<2.2-dev",
        "components/modernizr": ">=2.6.2,<2.7-dev"
    },
    "autoload": {
        "psr-0": {
            "HBAgency\\": [
                "system/modules/zurb_foundation/library/"
            ]
        }
    },
    "extra": {
        "contao": {
            "sources": {
                "": "system/modules/zurb_foundation"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist"
    }
}